{
  "name": "store_info",
  "version": "0.1.0",
  "private": true,
  "productName": "STORE INFO",
  "main": "electron/main.js",
  "proxy": "http://localhost:3000",
  "author": "Akhil Kumar",
  "homepage": "./",
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.2.0",
    "electron-is-dev": "^1.2.0",
    "fluent-ffmpeg": "^2.1.0",
    "react": "^16.13.1",
    "react-csv": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-select": "^3.1.0",
    "react-table": "^7.2.2",
    "styled-components": "^4.3.2",
    "universal-cookie": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-electron": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "dev": "concurrently \"npm run start\" \"npx json-server --watch db.json --port 3001\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "build-electron": "mkdir build/src && robocopy electron build/electron /S & robocopy src/shared build/src/shared /S",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "webpack-cli": "^3.3.12"
  },
  "build": {
    "appId": "com.example.stock_info",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
